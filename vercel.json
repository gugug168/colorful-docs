{
  "version": 2,
  "functions": {
    "api/check-task.js": {
      "maxDuration": 10
    },
    "app.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/check-task",
      "dest": "/api/check-task.js"
    },
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ]
} 