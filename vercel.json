{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/check-task",
      "dest": "/api/check-task.js"
    },
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ],
  "functions": {
    "api/check-task.js": {
      "maxDuration": 10
    }
  }
} 